# Viessmann API credentials - REPLACE THESE WITH YOUR ACTUAL CREDENTIALS
VIESSMANN_USER=your_viessmann_email@example.com
VIESSMANN_PASSWORD=your_viessmann_password

# Client ID for Viessmann API (default should work for most cases)
CLIENT_ID=vicare-app

# Data storage configuration
DATA_DIR=~/heatpump_data

# Polling frequency in minutes
POLLING_INTERVAL=15

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOGLEVEL=DEBUG